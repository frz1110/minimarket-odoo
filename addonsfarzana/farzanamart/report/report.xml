<?xml version='1.0' encoding='utf-8'?>
<odoo>

    <record id="report_penjualan_pdf" model="ir.actions.report">
        <field name="name">Penjualan Pdf</field>
        <field name="model">farzanamart.penjualan</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">farzanamart.report_penjualan_pdf</field>
        <field name="report_file">farzanamart.report_penjualan_pdf</field>
        <field name="binding_model_id" ref="model_farzanamart_penjualan"/>
