<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <!-- farzanamart.person tree view -->
    <record id="farzanamart_person_view_tree" model="ir.ui.view">
        <field name="name">farzanamart.person.view.tree</field>
        <field name="model">farzanamart.person</field>
        <field name="arch" type="xml">
            <tree>
                <!-- Add your fields here -->
                <field name="name"/>
                <field name="alamat"/>
                <field name="tgl_lahir"/>
            </tree>
        </field>
    </record>
    
    <!-- farzanamart.person form view -->
    <record id="farzanamart_person_view_form" model="ir.ui.view">
        <field name="name">farzanamart.person.view.form</field>
        <field name="model">farzanamart.person</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group name="nama" string="Nama">
                        <!-- Add your fields here -->
                        <field name="name" />
                    </group>
                    <group name="data" string="Data">
                        <field name="alamat" />
                        <field name="tgl_lahir" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>